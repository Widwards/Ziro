<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="ZiroServerWcfServiceLibrary.ZiroMainService" >
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8081/ZiroMainService"/>
          </baseAddresses>
        </host>
      </service>
    </services>
  <behaviors>
    <serviceBehaviors>
      <behavior>
        <serviceMetadata httpGetEnabled="true"/>
      </behavior>
    </serviceBehaviors>
  </behaviors>
  </system.serviceModel>
</configuration>